generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Company {
    id             String @id @default(cuid())
    userId         String
    auth_code      String
    company_number String
    company_name   String

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
